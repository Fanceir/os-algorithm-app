{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.ts",
            "use": "@vercel/vite"
        }
    ],
    "routes": [
        {
            "src": "/(.*).js",
            "dest": "/dist/$1.js"
        },
        {
            "src": "/(.*).css",
            "dest": "/dist/$1.css"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}